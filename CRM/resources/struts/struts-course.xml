<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="course" namespace="/" extends="common">
        <action name="courseAction_*" class="courseAction" method="{1}">
            <result name="findAll">/WEB-INF/pages/coursetype/listCourse.jsp</result>
            <result name="editUI">/WEB-INF/pages/coursetype/addOrEditCourse.jsp</result>
            <result name="addOrEdit" type="redirectAction">courseAction_findAll</result>
        </action>
    </package>

</struts>